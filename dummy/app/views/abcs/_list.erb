<%= turbo_frame_tag "abcs-list"   do %>
  <div class="container-fluid scaffold-list">
    
      <h4>
        Apples
      </h4>
    

    <%= render partial: "abcs/new_button", locals: {} %><br />

    
      
        <div class="row scaffold-heading-row">
          <div class='col-md-1'>Name</div>
          

          

          <div class=' scaffold-col-heading scaffold-col-heading-buttons col-md-2' >

          </div>
        </div>
      

      <% if abcs.empty? %>
    <div>
        None
    </div>
      <% end %>
      <% abcs.each do |abc| %>
        <%= render partial: 'abcs/line', locals: {abc: abc}
            .merge(defined?(nested_for) ? {nested_for: nested_for} : {})
      
        %>
      <% end %>
    <% if abcs.respond_to?(:total_pages) %><%= paginate(abcs) %> <% end %>
  
  </div>
<% end %>
